<h1>Eat-Da-Burger!</h1>

<ul>
	{{#each burgers}}
	<li>
		<p>{{burger}}</p>
		<form action={{url}} method="GET">
			<button>Devour it!</button>
		</form>

	</li>
	{{/each}}
</ul>

<form name="add_burger" action="/" method="POST">
	<input type="text" id="new_burger">

	<button type="submit">Add burger</button>
</form>
